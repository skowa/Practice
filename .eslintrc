{
    "extends": "airbnb-base",
    "rules": {
        "comma-dangle": 0,
        "linebreak-style": 0,
        "prefer-template": 0,
        "no-console": 0,
        "no-use-before-define": 0,
        "no-param-reassign": 0,
        "no-plusplus": 0,
        "eqeqeq": 0,
        "no-path-concat": 0,
        "max-len": 0,
        "prefer-destructuring": 0,
        "no-restricted-globals": 0,
        "no-alert": 0,
        "no-undef": 0
    }
}